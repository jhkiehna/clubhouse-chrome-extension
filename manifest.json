{
  "name": "Clubhouse-Github Story Linker",
  "manifest_version": 2,
  "version": "0.1",
  "description": "Description.",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*/*/compare*"],
      "js": ["js/script.js"]
    }
  ],
  "background": {
    "scripts": ["js/api.js"],
    "persistent": false
  }
}
